<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CSV Data</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }
        table, th, td {
            border: 1px solid black;
        }
        th, td {
            padding: 8px;
            text-align: left;
        }
    </style>
</head>
<body>
    <h1>CSV Data</h1>
    
    <% if (data.length > 0) { %>
        <table>
            <thead>
                <tr>
                    <% Object.keys(data[0]).forEach((key) => { %>
                        <th><%= key %></th>
                    <% }); %>
                </tr>
            </thead>
            <tbody>
                <% data.forEach((row) => { %>
                    <tr>
                        <% Object.values(row).forEach((value) => { %>
                            <td><%= value %></td>
                        <% }); %>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    <% } else { %>
        <p>No data found.</p>
    <% } %>
</body>
</html>
